	"private": true,
	"devDependencies": {
		"concat-bin": "^1.0.1",
		"rimraf": "^2.2.8",
		"vui-breadcrumbs": "^0.6.0",
		"vui-button": "^0.7.0",
		"vui-field": "^0.6.0",
		"vui-focus": "^0.7.0",
		"vui-icons": "^0.5.0",
		"vui-input": "^0.6.0",
		"vui-link": "^0.6.0",
		"vui-list": "^0.4.0",
		"vui-offscreen": "^0.2.0",
		"vui-typography": "^0.5.0"
